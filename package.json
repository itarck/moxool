{
  "name": "Mox.Astronomy",
  "version": "0.1.0-alpha",
  "author": "Mox",
  "description": "An explorable model of astronomy",
  "license": "UNLICENSED",
  "scripts": {
    "server/dev": "lein repl",
    "web/dev": "clj -A:shadow-cljs watch webapp",
    "web/compile": "clj -A:shadow-cljs compile webapp",
    "web/release": "clj -A:shadow-cljs release webapp",
    "web/clean": "rm -rf resource/public/js/* && rm -rf target",
    "desktop/dev": "clj -A:shadow-cljs watch desktop electron",
    "desktop/start": "cd desktop && yarn start",
    "desktop/prepare": "bb bbscripts/desktop-prepare.clj",
    "desktop/install": "cd desktop && yarn",
    "desktop/compile": "clj -A:shadow-cljs release desktop electron",
    "desktop/make": "cd desktop && yarn make",
    "desktop/clean": "rm -rf desktop/js && rm -rf desktop/main.js && rm -rf desktop/out", 
    "desktop/purge": "rm -rf desktop"
  },
  "devDependencies": {
    "css-loader": "^5.0.1",
    "shadow-cljs": "^2.12.2",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "3.2.10",
    "@react-three/drei": "^3.11.2",
    "camera-controls": "^1.27.1",
    "react": "17.0.1",
    "react-audio-player": "0.14.0",
    "react-dom": "17.0.1",
    "react-h5-audio-player": "^3.5.0",
    "react-refresh": "0.8.1",
    "react-three-fiber": "^5.3.21",
    "three": "^0.128.0",
    "xregexp": "^5.0.1"
  }
}
